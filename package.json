{
	"name": "@bangbu/react-intl-formatted-token",
	"description": "Components that format data for display",
	"version": "0.0.1-alpha.5",
	"type": "module",
	"main": "dist/cjs/index.js",
	"types": "dist/types/index.d.ts",
	"repository": {
		"url": "https://github.com/bangbu10820/react-intl-formatted-token"
	},
	"exports": {
		"import": "./dist/esm/index.js",
