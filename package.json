	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@formatjs/ecma402-abstract": "^2.3.4",
		"decimal.js": "^10.5.0"
	},
	"peerDependencies": {
		"react": "16 || 17 || 18 || 19",
		"typescript": "^5.8.3",
